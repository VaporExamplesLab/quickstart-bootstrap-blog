<body class="d-flex flex-column h-100">

<!-- <header> -->
<!-- Fixed navbar -->
<!-- navbar{-dark|-light|-primary} -->
<!-- bg{-dark|-light|-primary} | style="background-color: #e3f2fd;" -->
<!--  navbar expansion: navbar-expand (always)|navbar-expand{-sm|-md|-lg|-xl} -->
<nav class="navbar navbar-expand-lg navbar-dark bg-dark fixed-top"> <!-- fixed-top|sticky-top -->
  <a class="navbar-brand align-items-center" href="/">
  <!-- strong>Blog</strong -->
  <img src="/images/blog-ffffff.svg" height="28">
  </a>

  <!-- Button toggles navigation bar for small screen sizes.
       Shows and hides the `navbarSupportedContent` section. -->
  <button class="navbar-toggler" type="button"
          data-toggle="collapse" data-target="#navbarMainMenusDiv"
          aria-controls="navbarMainMenusDiv"
          aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>

  <!-- Collapsable menu 'div' section. -->
  <div class="collapse navbar-collapse" id="navbarMainMenusDiv">
    <!-- Define a list of navigation `nav-item` links. -->
    <ul class="navbar-nav mr-auto">
      <!-- Uses vapor leaf 'if' statement to set 'active' menu -->
      <!-- Home|Category>|Recent>|Workflow -->
      <li class="nav-item #if(title == "Home"){active}">
        <a href="/" class="nav-link">Home</a>
      </li>
      <!-- Category -->
      <li class="nav-item dropdown">
        <a class="nav-link dropdown-toggle" href="#" id="navbarCategoryDropdown"
           data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">Category</a>
        <div class="dropdown-menu" aria-labelledby="navbarCategoryDropdown">
            #embed("BaseCategory")
        </div>
      </li>
      <!-- Recent -->
      <li class="nav-item dropdown">
        <a class="nav-link dropdown-toggle" href="#" id="navbarRecentDropdown"
           data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">Recent</a>
        <div class="dropdown-menu" aria-labelledby="navbarRecentDropdown">
            #embed("BaseRecent")
        </div>
      </li>
      <!-- Reference -->
      <li class="nav-item dropdown">
        <a class="nav-link dropdown-toggle" href="#" id="navbarReferenceDropdown"
           data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">Reference</a>
        <div class="dropdown-menu" aria-labelledby="navbarReferenceDropdown">
          <a class="dropdown-item" href="/reference_bootstrap">Bootstrap Elements</a>
          <a class="dropdown-item" href="/reference_generated">Generated Elements</a>
          <a class="dropdown-item" href="/reference_workflow">Workflow</a>
        </div>
      </li>
    </ul>
    <form class="form-inline"> <!-- mt-2 mt-md-0 -->
      <input class="form-control-sm mr-sm-2" type="text" placeholder="Search" aria-label="Search">
      <!-- btn-outline-success|btn-secondary my-2 my-sm-0 -->
      <button class="btn btn-outline-success btn-sm" type="submit">Search</button>
    </form>
  </div> <!-- end navbarMainMenusDiv -->
</nav>
<!-- </header> -->

<!-- Begin page content -->
<!-- mt-3: margin top `1rem` -->
<!-- flex-{grow|shrink}-{*|0|1}
     flex-shrink-0: do not shrink keep. bottom menu in place -->
<main role="main" class="flex-shrink-0">
<div class="container mt-0">
    #get(body)
</div>
</main>


